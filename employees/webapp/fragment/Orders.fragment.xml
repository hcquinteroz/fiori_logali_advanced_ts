<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">

    <Table items="{northwind>Orders}">
        <columns>
            <Column>
                <Text text="{i18n>orderID}" />
            </Column>
            <Column>
                <Text text="{i18n>freight}" />
            </Column>
            <Column>
                <Text text="{i18n>shipAddress}" />
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{northwind>OrderID}"></Text>
                    <Text text="{northwind>Freight}"></Text>
                    <Text text="{northwind>ShipAddress}"></Text>
                </cells>
            </ColumnListItem>
        </items>
    </Table>

</core:FragmentDefinition>
